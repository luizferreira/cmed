                </tbody>
            </table>
        </div>

        <p tal:condition="context/event_url|nothing">
            <a class="url"
               href="event_url"
               i18n:translate="event_more_information"
               tal:define="field python:context.getField('eventUrl');"
                        id="parent-fieldname-eventUrl"
                        tal:attributes="href context/event_url">
