<html xmlns="http://www.w3.org/1999/xhtml"
	  xml:lang="en"
	  lang="en"
	  xmlns:tal="http://xml.zope.org/namespaces/tal"
	  xmlns:metal="http://xml.zope.org/namespaces/metal"
	  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
	  metal:use-macro="here/main_template/macros/master"
	  i18n:domain="cmfuemr">

  <metal:head fill-slot="top_slot">
	<tal:block define="macro edit_macros/topslot | nothing" condition="macro">
	    <!--<metal:block use-macro="macro" />-->
	</tal:block>
  </metal:head>
  
  <metal:head fill-slot="javascript_head_slot">
<!--	<tal:block define="macro here/archetypes_custom_js/macros/javascript_head | nothing"
			   condition="macro">
	  <metal:block use-macro="macro" />
	</tal:block>-->
  </metal:head>

  <body>
	<metal:fill fill-slot="main">
	  <metal:main define-macro="main">
        <h1> Troca de Assinatura Eletr√¥nica </h1>
        <br/>
        <form tal:attributes="action template/id">
            <input type="hidden" name="form.submitted" value="1" />
                <table>
                    <tr>
                        <td>Senha atual:</td><td><input type="text" name="act"/></td>
                    </tr>
                    <tr>
                        <td>Nova senha:</td><td><input type="password" name="new"/></td>
                    </tr>
                    <tr>
                        <td>Confirmar senha:</td><td><input type="password" name="conf"/></td>
                    </tr>
                    <tr>
                        <td>
                            <br/>
                            <input type="submit" id="sign" value="Trocar" class="context" name="form.button.sign" i18n:attributes="value"/>
                            <input type="submit" id="cancel" value="cancel" class="context" name="form.button.cancel" i18n:attributes="value"/>
                        </td>
                    </tr>
                </table>
          </form>
	  </metal:main>
	</metal:fill>
  </body>

</html>
