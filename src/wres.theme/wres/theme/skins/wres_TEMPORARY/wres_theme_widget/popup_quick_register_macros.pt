<metal:popup_quick_register_confirmation define-macro="confirmation">
<html tal:define="field_id request/field_id;
                  field_name string:${field_id}:list;
                  name context/Title;
                  fields python:context.Schema().filterFields(required=1);
				  utool nocall:here/portal_url;
				  portal_url utool;
				  portal utool/getPortalObject"
				  i18n:domain="cmfuemr">
<head>
	<script type="text/javascript" charset="iso-8859-1"
              src="http://localhost/bla/uemr_widgets/js/testebotoeswidget.js"
              tal:attributes="src string:${portal_url}/buildingblockwidget.js">
    </script> 

	<link rel="stylesheet" type="text/css" href="popup_quick_register.css" />

</head>

<body>
    <br />
    <br />
	<span i18n:translate="description_object_created">
		<b tal:content="name" >Object</b> registrado. <br /><br />
	</span>
    <metal:extra_text_fields define-slot="extra_text_fields" />

	<fieldset>
	    <legend tal:content="string: Dados de ${name}"></legend>
        <tal:fields define="field_macro field_macro | here/widgets/field/macros/view;
                            mode string:view"
        			repeat="field fields">
          <strong><tal:print content="python:field.widget.Label(context)" /></strong>: 
          <metal:fieldMacro use-macro="python:context.widget(field.getName(), mode='view')" />
          <br />
	      <!--  metal:fieldMacro use-macro="field_macro" />  -->
        </tal:fields>
		<div align="right">
		    <input type="button" value="Select" class="standalone add" id="select" i18n:attributes="value"
		           tal:attributes="onclick string:populateOpenerField('${field_name}', '${context/Title}', '${context/absolute_url_path}');" />
	    </div>	
	</fieldset>
	
	
</body>
</html>
</metal:popup_quick_register_confirmation>
